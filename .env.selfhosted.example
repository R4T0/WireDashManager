
# Configurações para WireDash Self-Hosted
# Use estas configurações quando executar com docker-compose.selfhosted.yml

# Configuração da aplicação para self-hosted
VITE_USE_LOCAL_SUPABASE=false
VITE_SELF_HOSTED=true

# Configuração do PostgreSQL (usado pelo Docker Compose)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=wireguard_manager
POSTGRES_PORT=5432

# Configuração da aplicação
NODE_ENV=production
PORT=80

# Configuração de logs
LOG_LEVEL=info

# Configuração de backup
BACKUP_RETENTION_DAYS=7
